pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building World'
            }
        }
        stage('deploy') {
            steps {
                echo 'Deploying World'
            }
        }
        stage('Test ') {
            steps {
                echo 'Testing World'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasng World'
            }
        }
    }
}
